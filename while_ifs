max = int (input ("NÃºmero de intentos: "))
counter = 0
while counter < max:
 num = int(input("Pick a number between 1 and 100:"))
 if num % 3 == 0 and num % 5 == 0:
    print("FizzBuzz")
 elif num % 3 == 0:
    print("Fizz")
 elif num % 5 == 0:
    print("Buzz")
 else:
    print("you lost")
 counter += 1
